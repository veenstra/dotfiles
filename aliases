alias a=alias
alias ls='ls -F'
alias la='ls -a'
alias ll='ls -la'
alias h=history
alias gr=grep
alias l=less
alias findx='find . -name "*.xml" -print0 | xargs -0 grep -H'
alias findj='find . -name "*.java" -print0 | xargs -0 grep -H'
alias findall='find . -type f -print0 | xargs -0 grep -H'
alias p=pushd
alias o=popd
alias null='cat > /dev/null'
alias make-air="make-dbg backlot_air_inner && make-dbg backlot_air_outer && kill-fcsh"
alias make-web="make-dbg ooyala_backlot_web && kill-fcsh"
